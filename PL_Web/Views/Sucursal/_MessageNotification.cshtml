<div class="modal" tabindex="-1" role="dialog" id="ModalMessage">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="card-title text-center">Notificación</h4>
            </div>
            <div class="modal-body">
                @if (ViewBag.result != null)
                {
                    if (ViewBag.result.Correct)
                    {
                        <div class="text-center mb-3">
                            <img src="/content/imagenes/verificado.png" style="width:15%" alt="Imagen alerta" />
                        </div>
                        <div class="alert alert-success text-center mb-4">
                            @ViewBag.succesMessage !
                        </div>
                    }
                    else
                    {
                        <div class="text-center mb-3">
                            <img src="/content/imagenes/advertencia.png" style="width:15%" alt="Imagen alerta" />
                        </div>
                        <div class="alert alert-danger text-center mb-4">
                            @ViewBag.result.ErrorMessage!
                        </div>
                    }
                }
            </div>
            <div class="modal-footer">
                @Html.ActionLink("Continuar", "GetAll", "Sucursal", new { area = "" }, htmlAttributes: new { @class = "btn btn-danger" })
            </div>
        </div>
    </div>
</div>

<script>
    $("#ModalMessage").modal()
</script>

